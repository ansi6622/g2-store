<%= form_tag new_session_path do %>
  <div class="form_container">
    <div class="form_input">
      <%= label_tag :email %>
      <%= text_field :session, :email %>
    </div>

    <div class="form_input">
      <%= label_tag :pasword %>
      <%= password_field :session, :password %>
    </div>

    <div class="button">
      <%= submit_tag "Login" %>
    </div>
<% end %>
</div>
